{
  "name": "@tc-lib/doc",
  "version": "0.0.4",
  "private": true,
  "description": "A react library developed with dumi",
  "homepage": "https://github.com/tpc-ht/tc-lib#readme",
  "bugs": {
    "url": "https://github.com/tpc-ht/tc-lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpc-ht/tc-lib.git"
  },
  "license": "MIT",
  "author": "tpc-ht <59469126+tpc-ht@users.noreply.github.com>",
  "main": "lib/components.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "boot": "lerna bootstrap",
    "build": "yarn build:c & yarn build:u",
    "build:c": "yarn workspace @tc-lib/components build",
    "build:d": "dumi build",
    "build:u": "yarn workspace @tc-lib/utils build",
    "clean": "lerna clean",
    "coverage": "jest --coverage",
    "dev": "dumi dev",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "publish": "lerna publish",
    "start": "npm run dev",
    "test": "jest",
    "test:init": "jest --init",
    "test:u": "yarn workspace @tc-lib/utils test"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@umijs/lint": "^4.0.0",
    "babel-jest": "^29.5.0",
    "dumi": "^2.0.2",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lerna": "^6.6.2",
    "less-plugin-npm-import": "^2.1.0",
    "os-locale": "^5.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "authors": [],
  "gitHead": "ee57d22280cdec8b3de7111c05427dd746eb1b59",
  "gitHooks": {
    "commit-msg": "node ./commitlint.js verify-commit"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
